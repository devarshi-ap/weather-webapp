{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "object-shorthand": "off",
        "no-constant-condition": "off",
        "no-use-before-define": ["error", {"functions": false, "classes": false}]
    }
}
